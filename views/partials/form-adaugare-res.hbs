<script>
  /**
  * Funcția are rolul de a adăuga un titlu într-o listă de control
  * De fapt, creează un nou element input și ul nou selector de limbă pentru elementul input:text nou creat
  * @param {String} idSource este elementul input:text în care a fost introdus un titlu
  * @param {String} idTraget este elementul ul în care apar titlurile
  */
  function addToList (idSource, idTarget) {
    document.querySelector(idSource).addEventListener('blur', (event) => {
      let insertion = document.querySelector(idTarget);
      let liElem = document.createElement('li');
      const cls = ["list-group-item","d-flex","justify-content-between","align-items-center"];
      liElem.classList.add(...cls);
      //TODO: Termină implementarea cândva, dacă mai este nevoie
    });
  }

  
  // era si asta cred in pachet. daca nu este, scuze... No problem”
  /**
   * Funcția are rolul de a face upload / switch din editor LIVRESQ in editor Standard
   */
  function livqUpload(){
      var livqTab = document.querySelector('#editor-option1');
      if(!livqTab) return;
      livqTab.click();

      var livqEditor = document.querySelector('.codex-editor__redactor');
      if(!livqEditor) return;
      livqEditor.click();

      var livqPlus = document.querySelector('.ce-toolbar__plus');
      if(!livqPlus) return;
      livqPlus.click();

      var livqAttach = document.querySelector('li[data-tool="attaches"]').click();
      if(!livqAttach) return;
      livqAttach.click();
  }

    // livresq and standard editors ~BEGIN
    // just to make sure it works; kept as js
    $(function(){
        // begin the editors ~ toggle editors
        toDefaultEditor();
        // end of editors ~ toggle editors

    });

    function toDefaultEditor(){
        if(document.getElementById("editor-default").style.display === 'none'){
          document.getElementById("editor-default").style.display = "block";
          document.getElementById("editor-livresq").style.display = "none";
        }
    }


    function toLivresqEditor(){
        if(document.getElementById("editor-livresq").style.display === 'none'){
          document.getElementById("editor-default").style.display = "none";
          document.getElementById("editor-livresq").style.display = "block";
        }
    }
    // livresq and standard editors ~~END

</script>

