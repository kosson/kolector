  "main": "app.js",
  "scripts": {
    "test": "node app.js | tap-spec",
    "start": "node app.js",
    "debug": "node --inspect=0.0.0.0 app.js",
    "dev": "nodemon -L --inspect=0.0.0.0 app.js",
    "kill": "kill $(lsof -t -i:8080) | exit 0"
  },
  "nodemonConfig": {
    "ignore": [
      "backup/*",
      "logs/*",
      "repo/*"
    ],
    "delay": 2500,
    "legacyWatch": true
